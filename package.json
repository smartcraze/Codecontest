{
  "name": "code-contest",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,}\"",
    "check-types": "turbo run check-types",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.18",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@inngest/agent-kit": "^0.9.0",
    "@notionhq/client": "^5.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "express-rate-limit": "^8.0.1",
    "inngest": "^3.40.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "notion-to-md": "^3.1.9",
    "react-hook-form": "^7.62.0",
    "redis": "^5.8.2",
    "totp-generator": "^1.0.0"
  }
}
